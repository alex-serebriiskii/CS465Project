@using React.AspNet
<!DOCTYPE html>
<html>
<head>
	@Html.Raw(ViewBag.HelmetTitle)
	<meta charset="utf-8" />
	<link rel="stylesheet" href="~/index.css" />
	@Html.Raw(ViewBag.ServerStyles)
	@Html.ReactGetStylePaths()
</head>
<body>
	@RenderBody()
	@Html.ReactGetScriptPaths()
	@Html.ReactInitJavaScript()
</body>
</html>
