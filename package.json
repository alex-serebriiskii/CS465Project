    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-emotion": "^9.2.12",
    "react-helmet": "^6.0.0",
    "react-jss": "^8.6.1",
    "react-router-dom": "^5.0.0",
    "react-select": "^3.0.4",
    "reactstrap": "^8.0.0",
    "styled-components": "^4.0.0",
    "url-search-params-polyfill": "^8.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
