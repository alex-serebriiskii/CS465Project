    "babel-eslint": "^10.1.0",
    "babel-loader": "8.0.6",
    "babel-runtime": "6.26.0",
    "date-fns": "^2.22.1",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "prettier": "^2.3.0",
    "rimraf": "3.0.2",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "2.2.0",
    "yup": "^0.32.9"
