{
  "name": "React.Sample.Webpack.CoreMvc",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf wwwroot/dist && webpack",
    "format": "npm run prettier && npm run import-sort && npm run eslint",
    "prettier": "prettier --write ./Content/**/*.{js,jsx}",
    "eslint": "eslint --fix ./Content/**/*.{js,jsx}",
    "import-sort": "import-sort --write ./Content/**/*.{js,jsx}",
    "docker-start": "docker-compose up --build --detach",
    "docker": "npm run docker-start && docker-compose exec app bash",
    "start": "dotnet run",
    "dev": "dotnet watch run"
  },
  "dependencies": {
    "@microsoft/signalr": "^5.0.6",
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
